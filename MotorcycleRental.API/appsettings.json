{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MotorcycleRental": "Host=pghost;Port=5432;Pooling=false;Database=MotorcycleRental;User Id=postgres;Password=MotorcycleRental@123"
  },
  "Jwt": {
    "Key": "Here should have a strong key dont use this key you can generate your own",
    "Issuer": "MotorcycleRental",
    "Audience": "CustomersRental"
  },
  "FilePath": "..\\MotorcycleRental.API",
  "Serilog": {
    "LevelSwitches": {
      "$controlSwitch": "Verbose"
    },
    "MinimumLevel": {
      "ControlledBy": "$controlSwitch"
    },
    "WriteTo": [
      {
        "Name": "PostgreSQL",
        "Args": {
          "connectionString": "MotorcycleRental",
          "tableName": "log_api",
          "schemaName": null,
          "needAutoCreateTable": true,
          "restrictedToMinimumLevel": "Information"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ]
  },
  "RabbitConnection": "localhost"
}
